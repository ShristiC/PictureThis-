{
  "expo": {
    "name": "First App Attempt",
    "slug": "newapp",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourcompany.yourappname",
      "config": {
        "googleMapsApiKey": "iOS Maps API key here"
      }
    },
    "android": {
      "package": "com.yourcompany.yourappname",
      "config": {
        "googleMaps": { "apiKey": "Android Maps API key here"}
      }
    }
  }
}
